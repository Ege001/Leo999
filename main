local WorldTab = Window:CreateTab("DÃ¼nya & Itemlar", 4483362458)

-- 1. MASTER GAMEPASS AKTÄ°VASYONU (TÃœM ID'LER DAHÄ°L)
MasterTab:CreateButton({
   Name = "ðŸ”¥ TÃœM GAMEPASSLERÄ° ADIMA TANIMLA",
   Callback = function()
      pcall(function()
          -- AllGamepasses.txt dosyasÄ±ndaki tÃ¼m kritik ID'ler 
          local allPassIds = {
              850049439, -- VIP [cite: 6]
