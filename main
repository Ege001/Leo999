-- 2. ADS BUTONU VE MENÃœSÃœ
MasterTab:CreateButton({
   Name = "ðŸ“¢ Ads MenÃ¼sÃ¼nÃ¼ Ã‡alÄ±ÅŸtÄ±r",
   Callback = function()
      pcall(function()
          local mainGui = lp.PlayerGui:FindFirstChild("MainGUIHandler", true)
          if mainGui then
              local ads = mainGui:FindFirstChild("AdStore", true)
              if ads then 
                  ads.Visible = not ads.Visible 
                  ads.Position = UDim2.new(0.5, 0, 0.5, 0)
              end
          end
      end)
   end,
})

-- 3. FLY VE NOCLIP (TAM KONTROL)
MovementTab:CreateToggle({
   Name = "UÃ§ma Modu (Fly)",
   CurrentValue = false,
   Callback = function(Value)
      flying = Value
      local char = lp.Character or lp.CharacterAdded:Wait()
      local root = char:WaitForChild("HumanoidRootPart")
      local hum = char:WaitForChild("Humanoid")
      if flying then
          bGyro = Instance.new("BodyGyro", root)
          bGyro.P = 9e4
          bGyro.maxTorque = Vector3.new(9e9, 9e9, 9e9)
          bVel = Instance.new("BodyVelocity", root)
          bVel.maxForce = Vector3.new(9e9, 9e9, 9e9)
          spawn(function()
              while flying do
                  runService.RenderStepped:Wait()
                  local moveVec = require(lp.PlayerScripts:WaitForChild("PlayerModule")):GetControls():GetMoveVector()
                  bVel.Velocity = (camera.CFrame.LookVector * -moveVec.Z + camera.CFrame.RightVector * moveVec.X) * flySpeed
                  bGyro.CFrame = camera.CFrame
                  hum.PlatformStand = true
              end
              if bGyro then bGyro:Destroy() end
              if bVel then bVel:Destroy() end
              hum.PlatformStand = false
          end)
      end
   end,
})

MovementTab:CreateSlider({
   Name = "UÃ§ma HÄ±zÄ±",
   Range = {10, 500},
   Increment = 10,
   CurrentValue = 50,
   Callback = function(Value) flySpeed = Value end,
})

MovementTab:CreateToggle({
   Name = "Noclip (Duvar GeÃ§me)",
   CurrentValue = false,
   Callback = function(Value)
      noclip = Value
      runService.Stepped:Connect(function()
         if noclip and lp.Character then
            for _, v in pairs(lp.Character:GetDescendants()) do
               if v:IsA("BasePart") then v.CanCollide = false end
            end
         end
      end)
   end,
})

-- 4. GÄ°ZLÄ° EÅžYA ALMA (REPLICATEDSTORAGE)
WorldTab:CreateButton({
   Name = "ðŸ“¦ TÃ¼m EÅŸyalarÄ± (Items) Envantere Ã‡ek",
   Callback = function()
      local count = 0
      for _, item in pairs(game:GetService("ReplicatedStorage"):GetDescendants()) do
          if item:IsA("Tool") then
              item:Clone().Parent = lp.Backpack
              count = count + 1
          end
      end
      Rayfield:Notify({Title = "EÅŸya AlÄ±cÄ±", Content = count .. " eÅŸya alÄ±ndÄ±!", Duration = 3})
   end,
})

-- AYARLAR
Window:CreateTab("Ayarlar").CreateButton({
   Name = "Kapat (Destroy)",
   Callback = function() Rayfield:Destroy() end,
})
