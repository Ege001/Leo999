local function GetMoneyGun()
    pcall(function()
        -- [cite: 24] ItemRemote Ã¼zerinden VIP MoneyGun tetikleme
        local remote = rs:FindFirstChild("ItemRemote", true) or rs:FindFirstChild("GiveItem", true)
        if remote then
            remote:FireServer("MoneyGun")
        end
        
        -- Replica iÃ§inden zorla Ã§ekme denemesi [cite: 17, 18]
        local tool = rs:FindFirstChild("MoneyGun", true)
        if tool then
            tool:Clone().Parent = lp.Backpack
            Rayfield:Notify({Title = "AI", Content = "MoneyGun Envantere Eklendi!", Duration = 3})
        end
    end)
end

ToolTab:CreateButton({Name = "ğŸ’µ MONEY GUN (VÄ°P) AL", Callback = function() GetMoneyGun() end})
