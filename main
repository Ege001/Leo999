-- Modern UI YÃ¼kleme (Sirius Rayfield)
local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local lp = game.Players.LocalPlayer
local runService = game:GetService("RunService")
local camera = workspace.CurrentCamera
local rs = game:GetService("ReplicatedStorage")
local ownerName = "EgeKazak_Real1" 

local Window = Rayfield:CreateWindow({
   Name = "Gemini Hub V26 | OWNER: " .. ownerName,
   LoadingTitle = "PlayersBag Verileri Ã‡Ã¶zÃ¼lÃ¼yor...",
   LoadingSubtitle = "EgeKazak_Real1 Mutlak Yetki",
   ConfigurationSaving = { Enabled = true, FolderName = "GeminiFinalV26" }
})

-- DEÄžÄ°ÅžKENLER
local flying = false
local noclip = false
local flySpeed = 50
local bGyro, bVel

-- 1. PLAYERSBAG VE TÃœM DEÄžERLERÄ° TRUE YAPMA (KRÄ°TÄ°K MODÃœL)
local function unlockEverything()
    pcall(function()
        -- PlayersBag KlasÃ¶rÃ¼nÃ¼ Bul ve TÃ¼m DeÄŸerleri Aktif Et
        local pb = lp:FindFirstChild("PlayersBag") or lp:WaitForChild("PlayersBag", 5)
        if pb then
            for _, val in pairs(pb:GetDescendants()) do
                if val:IsA("BoolValue") then
                    val.Value = true
                elseif val:IsA("NumberValue") or val:IsA("IntValue") then
                    val.Value = 1 -- 1 Genellikle True demektir
                end
            end
        end

        -- AllGamepasses.txt ID Bypass
        for _, v in pairs(getgc(true)) do
            if type(v) == "table" then
                if rawget(v, "AllPassesFetched") ~= nil then v.AllPassesFetched = true end
                if rawget(v, "__GAMEPASS") ~= nil then v.__GAMEPASS = true end
                if rawget(v, "Premium") ~= nil then v.Premium = true end
                if rawget(v, "VIP") ~= nil then v.VIP = true end
            end
        end

        -- MarketPlace Hook
        local mt = getrawmetatable(game)
        setreadonly(mt, false)
        local old = mt.__index
        mt.__index = newcclosure(function(t, k)
            if k == "UserOwnsGamePassAsync" or k == "PlayerOwnsAsset" then return true end
            return old(t, k)
        end)

        -- Ads Button & Menu Fix
        local MainGUI = lp.PlayerGui:FindFirstChild("MainGUIHandler", true)
        if MainGUI then
            local ads = MainGUI:FindFirstChild("AdStore", true)
            if ads then 
                ads.Visible = true 
                ads.Position = UDim2.new(0.5, 0, 0.5, 0)
            end
            for _, b in pairs(MainGUI:GetDescendants()) do
                if b.Name == "Ads" or b.Name == "Sponsor" then
                    b.Visible = true
                    b.Transparency = 0
                end
            end
        end
    end)
end

-- SEKMELER
local MasterTab = Window:CreateTab("Owner Control", 4483362458)
local ViewTab = Window:CreateTab("EÅŸya SeÃ§ici (View)", 4483362458)
local MoveTab = Window:CreateTab("Hareket (Fly)", 4483362458)
local SettingsTab = Window:CreateTab("Ayarlar", 4483362458)

-- MASTER BUTON
MasterTab:CreateButton({
   Name = "ðŸ‘‘ OWNER ALL GAMEPASS (PlayersBag Unlock)",
   Callback = function()
       unlockEverything()
       Rayfield:Notify({
           Title = "SÄ°STEM ONAYI",
           Content = "PlayersBag Ã§Ã¶zÃ¼ldÃ¼! EgeKazak_Real1 iÃ§in tÃ¼m kilitler aÃ§Ä±ldÄ±.",
           Duration = 5
       })
   end,
})

-- EÅžYA SEÃ‡Ä°CÄ° (VIEW)
local uniqueItems = {}
for _, item in pairs(rs:GetDescendants()) do
    if item:IsA("Tool") then table.insert(uniqueItems, item.Name) end
end

ViewTab:CreateDropdown({
   Name = "TÃ¼m Brookhaven EÅŸyalarÄ±",
   Options = uniqueItems,
   CurrentOption = "",
   Callback = function(Option)
       local target = rs:FindFirstChild(Option, true)
       if target and target:IsA("Tool") then
           target:Clone().Parent = lp.Backpack
       end
   end,
})

-- FLY SÄ°STEMÄ°
MoveTab:CreateToggle({
   Name = "UÃ§ma (Fly)",
   CurrentValue = false,
   Callback = function(Value)
      flying = Value
      local char = lp.Character or lp.CharacterAdded:Wait()
      local root = char:WaitForChild("HumanoidRootPart")
      if flying then
          bGyro = Instance.new("BodyGyro", root)
          bGyro.P = 9e4; bGyro.maxTorque = Vector3.new(9e9, 9e9, 9e9)
          bVel = Instance.new("BodyVelocity", root)
          bVel.maxForce = Vector3.new(9e9, 9e9, 9e9)
          spawn(function()
              while flying do
                  runService.RenderStepped:Wait()
                  local moveVec = require(lp.PlayerScripts:WaitForChild("PlayerModule")):GetControls():GetMoveVector()
                  bVel.Velocity = (camera.CFrame.LookVector * -moveVec.Z + camera.CFrame.RightVector * moveVec.X) * flySpeed
                  bGyro.CFrame = camera.CFrame
                  char.Humanoid.PlatformStand = true
              end
              if bGyro then bGyro:Destroy() end
              if bVel then bVel:Destroy() end
              char.Humanoid.PlatformStand = false
          end)
      end
   end,
})

MoveTab:CreateSlider({
   Name = "HÄ±z", Range = {10, 500}, Increment = 10, CurrentValue = 50,
   Callback = function(Value) flySpeed = Value end,
})

-- DESTROY
SettingsTab:CreateButton({
   Name = "Scripti Kapat (Clean Destroy)",
   Callback = function() Rayfield:Destroy() end,
})
