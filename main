-- Modern UI YÃ¼kleme (Sirius Rayfield)
local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local lp = game.Players.LocalPlayer
local runService = game:GetService("RunService")
local camera = workspace.CurrentCamera
local myName = lp.Name

local Window = Rayfield:CreateWindow({
   Name = "Gemini Hub V20 | THE FINAL GOD",
   LoadingTitle = "TÃœM KÄ°LÄ°TLER KIRILIYOR...",
   LoadingSubtitle = "HoÅŸ geldin, " .. myName,
   ConfigurationSaving = { Enabled = true, FolderName = "GeminiFinalV20" }
})

-- DEÄžÄ°ÅžKENLER
local flying = false
local noclip = false
local flySpeed = 50
local bGyro, bVel

-- SEKMELER
local GodTab = Window:CreateTab("GOD MODE (TÃ¼mÃ¼nÃ¼ AÃ§)", 4483362458)
local CharacterTab = Window:CreateTab("Karakter (Fly/Noclip)", 4483362458)
local MovementTab = Window:CreateTab("HÄ±z & ZÄ±plama", 4483362458)
local VehicleTab = Window:CreateTab("AraÃ§ & Modifiye", 4483362458)

-- 1. EFSANEVÄ° "TÃœMÃœNÃœ AÃ‡" SÄ°STEMÄ°
GodTab:CreateButton({
   Name = "ðŸ”¥ TÃœMÃœNÃœ AKTÄ°F ET (VIP + Premium + Ads + Items)",
   Callback = function()
      pcall(function()
          -- SÄ°STEM 1: BÃœTÃœN GAMEPASS VE ITEM ID'LERÄ°NÄ° HAFIZAYA ENJEKTE ET
          for _, v in pairs(getgc(true)) do
              if type(v) == "table" then
                  -- AllGamepasses.txt mantÄ±ÄŸÄ±ndaki tÃ¼m boolean deÄŸerleri true yap
                  if rawget(v, "AllPassesFetched") ~= nil then v.AllPassesFetched = true end
                  if rawget(v, "__GAMEPASS") ~= nil then v.__GAMEPASS = true end
                  if rawget(v, "Premium") ~= nil then v.Premium = true end
                  if rawget(v, "VIP") ~= nil then v.VIP = true end
                  if rawget(v, "VehicleSpeed") ~= nil then v.VehicleSpeed = true end
                  if rawget(v, "Unlocked") ~= nil then v.Unlocked = true end
                  if rawget(v, "LandUnlocked") ~= nil then v.LandUnlocked = true end
                  if rawget(v, "Penthouse") ~= nil then v.Penthouse = true end
              end
          end

          -- SÄ°STEM 2: MARKETPLACE SORGULARINI TAMAMEN ELE GEÃ‡Ä°R
          local mt = getrawmetatable(game)
          setreadonly(mt, false)
          local old = mt.__index
          mt.__index = newcclosure(function(t, k)
              if k == "UserOwnsGamePassAsync" or k == "PlayerOwnsAsset" or k == "UserOwnsBundle" then
                  return true
              end
              return old(t, k)
          end)

          -- SÄ°STEM 3: ADS VE SPONSOR MENÃœLERÄ°NÄ° ZORLA AÃ‡
          local MainGUI = lp.PlayerGui:FindFirstChild("MainGUIHandler", true)
          if MainGUI then
              local adStore = MainGUI:FindFirstChild("AdStore", true)
              if adStore then 
                  adStore.Visible = true 
                  adStore.Position = UDim2.new(0.5, 0, 0.5, 0)
              end
              -- Sponsorluk (Ads) etiketlerini her yerde aktifleÅŸtir
              for _, child in pairs(MainGUI:GetDescendants()) do
                  if child.Name == "Ads" or child.Name == "Sponsor" or child.Name == "AdButton" then
                      child.Visible = true
                      child.Transparency = 0
                  end
              end
          end
          
          Rayfield:Notify({Title = "SÄ°STEM", Content = "TÃœM BROOKHAVEN KÄ°LÄ°TLERÄ° AÃ‡ILDI! Her ÅŸey senin.", Duration = 7})
      end)
   end,
})

-- 2. KARAKTER Ã–ZELLÄ°KLERÄ° (FLY, NOCLIP, SPEED, JUMP)
CharacterTab:CreateToggle({
   Name = "UÃ§ma Modu (Fly)",
   CurrentValue = false,
   Callback = function(Value)
      flying = Value
      local char = lp.Character or lp.CharacterAdded:Wait()
      local root = char:WaitForChild("HumanoidRootPart")
      local hum = char:WaitForChild("Humanoid")
      if flying then
          bGyro = Instance.new("BodyGyro", root)
          bGyro.P = 9e4
          bGyro.maxTorque = Vector3.new(9e9, 9e9, 9e9)
          bVel = Instance.new("BodyVelocity", root)
          bVel.maxForce = Vector3.new(9e9, 9e9, 9e9)
          spawn(function()
              while flying do
                  runService.RenderStepped:Wait()
                  local moveVec = require(lp.PlayerScripts:WaitForChild("PlayerModule")):GetControls():GetMoveVector()
                  bVel.Velocity = (camera.CFrame.LookVector * -moveVec.Z + camera.CFrame.RightVector * moveVec.X) * flySpeed
                  bGyro.CFrame = camera.CFrame
                  hum.PlatformStand = true
              end
              if bGyro then bGyro:Destroy() end
              if bVel then bVel:Destroy() end
              hum.PlatformStand = false
          end)
      end
   end,
})

CharacterTab:CreateSlider({
   Name = "UÃ§ma HÄ±zÄ±",
   Range = {10, 500},
   Increment = 10,
   CurrentValue = 50,
   Callback = function(Value) flySpeed = Value end,
})

CharacterTab:CreateToggle({
   Name = "Noclip (Duvar GeÃ§me)",
   CurrentValue = false,
   Callback = function(Value)
      noclip = Value
      runService.Stepped:Connect(function()
         if noclip and lp.Character then
            for _, v in pairs(lp.Character:GetDescendants()) do
               if v:IsA("BasePart") then v.CanCollide = false end
            end
         end
      end)
   end,
})

MovementTab:CreateSlider({
   Name = "YÃ¼rÃ¼me HÄ±zÄ±",
   Range = {16, 500},
   CurrentValue = 16,
   Callback = function(Value) lp.Character.Humanoid.WalkSpeed = Value end,
})

MovementTab:CreateSlider({
   Name = "ZÄ±plama GÃ¼cÃ¼",
   Range = {50, 500},
   CurrentValue = 50,
   Callback = function(Value) lp.Character.Humanoid.JumpPower = Value end,
})

VehicleTab:CreateToggle({
   Name = "AraÃ§ SÄ±nÄ±rlarÄ±nÄ± KaldÄ±r",
   CurrentValue = false,
   Callback = function(Value)
      _G.CarFast = Value
      spawn(function()
         while _G.CarFast do
            pcall(function()
                for _, v in pairs(workspace.Vehicles:GetChildren()) do
                    local seat = v:FindFirstChildWhichIsA("VehicleSeat", true)
                    if seat and seat.Occupant and seat.Occupant.Parent == lp.Character then
                        seat.MaxSpeed = 450
                        seat.Torque = 25000
                    end
                end
            end)
            task.wait(0.2)
         end
      end)
   end,
})

-- AYARLAR
Window:CreateTab("Ayarlar").CreateButton({
   Name = "Kapat (Destroy)",
   Callback = function() Rayfield:Destroy() end,
})
