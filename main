-- ğŸŒ€ 1. PROFESYONEL LOADING SCREEN (HATA VERMEZ)
local function FinalLoading()
    local sg = Instance.new("ScreenGui", game.CoreGui)
    local frame = Instance.new("Frame", sg)
    frame.Size = UDim2.new(1, 0, 1, 0)
    frame.BackgroundColor3 = Color3.fromRGB(5, 5, 10)
    
    local txt = Instance.new("TextLabel", frame)
    txt.Size = UDim2.new(0, 500, 0, 100)
    txt.Position = UDim2.new(0.5, -250, 0.45, 0)
    txt.Text = "SOVEREIGN OVERRIDE V44\nBrookhaven Verileri Ä°ÅŸleniyor..."
    txt.TextColor3 = Color3.fromRGB(0, 170, 255)
    txt.Font = Enum.Font.GothamBold
    txt.TextSize = 30
    txt.BackgroundTransparency = 1
    
    task.wait(2)
    sg:Destroy()
end
pcall(FinalLoading)

