-- 1. BA≈ûLANGI√á EKRANI
local function Startup()
    local sg = Instance.new("ScreenGui", game.CoreGui)
    local f = Instance.new("Frame", sg)
    f.Size = UDim2.new(1, 0, 1, 0)
    f.BackgroundColor3 = Color3.fromRGB(5, 5, 12)
    
    local t = Instance.new("TextLabel", f)
    t.Size = UDim2.new(1, 0, 0, 100)
    t.Position = UDim2.new(0.5, -250, 0.45, 0)
    t.AnchorPoint = Vector2.new(0.5, 0.5)
    t.Text = "SOVEREIGN ULTIMATE V46\nHazƒ±rlanƒ±yor..."
    t.TextColor3 = Color3.fromRGB(0, 200, 255)
    t.Font = Enum.Font.GothamBold
    t.TextSize = 35
    t.BackgroundTransparency = 1
    
    task.wait(1.2)
    sg:Destroy()
end
pcall(Startup)

-- 2. DEƒûƒ∞≈ûKENLER & SERVƒ∞SLER
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local UserInputService = game:GetService("UserInputService")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local lp = Players.LocalPlayer
local Camera = workspace.CurrentCamera

_G.Speed = 16
_G.Jump = 50
_G.FOV = 70
_G.SpeedEnabled = false
_G.JumpEnabled = false
_G.Fly = false
_G.Noclip = false
_G.InfiniteJump = false

-- 3. RAYFIELD UI Y√úKLEME
local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
local Window = Rayfield:CreateWindow({
   Name = "üëë SOVEREIGN V46 | ULTIMATE",
   LoadingTitle = "Brookhaven Edition",
   LoadingSubtitle = "by EgeKazak_Real1",
   Theme = "DarkBlue",
   ConfigurationSaving = { Enabled = false }
})

local MainTab = Window:CreateTab("üè† Ana Panel", 4483362458)
local MoveTab = Window:CreateTab("‚ö° Hareket", 4483362458)
local TPTab = Window:CreateTab("‚úàÔ∏è I≈üƒ±nlanma", 4483362458)
local ToolTab = Window:CreateTab("üì¶ E≈üyalar", 4483362458)
local ExtraTab = Window:CreateTab("‚ú® Ekstralar", 4483362458)

-- 4. ANA PANEL (FULL ADS & RESET)
MainTab:CreateButton({
    Name = "üì¢ T√úM REKLAMLARI VE FRAMELERƒ∞ A√á",
    Callback = function()
        pcall(function()
            for _, v in pairs(lp.PlayerGui:GetDescendants()) do
                if v:IsA("GuiObject") then
                    local n = v.Name:lower()
                    if n:find("ad") or n:find("frame") or n:find("promo") or n:find("advert") then
                        v.Visible = true
                        -- ƒ∞√ßindeki t√ºm elemanlarƒ± da g√∂ster
                        for _, child in pairs(v:GetDescendants()) do
                            if child:IsA("GuiObject") then child.Visible = true end
                        end
                    end
                end
            end
            -- Remote tetikleme (Brookhaven √∂zel reklamlarƒ± i√ßin)
            local rem = ReplicatedStorage:FindFirstChild("TriggerAd", true) or ReplicatedStorage:FindFirstChild("ShowAds", true)
            if rem and rem:IsA("RemoteEvent") then rem:FireServer() end
        end)
    end,
})

MainTab:CreateButton({
    Name = "üîÑ Ayarlarƒ± Sƒ±fƒ±rla",
    Callback = function()
        _G.Speed = 16; _G.Jump = 50; _G.FOV = 70; _G.SpeedEnabled = false; _G.JumpEnabled = false
        _G.Fly = false; _G.Noclip = false; Camera.FieldOfView = 70
        Rayfield:Notify({Title = "Sƒ±fƒ±rlandƒ±", Content = "T√ºm deƒüerler normale d√∂nd√º.", Duration = 2})
    end,
})

-- 5. HAREKET (SAYI + ON/OFF + PLUS/MINUS)
MoveTab:CreateSection("Hƒ±z Kontrol√º")
MoveTab:CreateToggle({ Name = "Hƒ±z Aktif", CurrentValue = false, Callback = function(v) _G.SpeedEnabled = v end })
MoveTab:CreateInput({
    Name = "Hƒ±z Deƒüeri", PlaceholderText = "16-1000",
    Callback = function(v) _G.Speed = tonumber(v) or 16 end,
})
MoveTab:CreateButton({ Name = "Hƒ±z +5", Callback = function() _G.Speed = _G.Speed + 5 end })
MoveTab:CreateButton({ Name = "Hƒ±z -5", Callback = function() _G.Speed = _G.Speed - 5 end })

MoveTab:CreateSection("Zƒ±plama Kontrol√º")
MoveTab:CreateToggle({ Name = "Zƒ±plama Aktif", CurrentValue = false, Callback = function(v) _G.JumpEnabled = v end })
MoveTab:CreateInput({
    Name = "Zƒ±plama Deƒüeri", PlaceholderText = "50-1000",
    Callback = function(v) _G.Jump = tonumber(v) or 50 end,
})
MoveTab:CreateButton({ Name = "Zƒ±plama +10", Callback = function() _G.Jump = _G.Jump + 10 end })
MoveTab:CreateButton({ Name = "Zƒ±plama -10", Callback = function() _G.Jump = _G.Jump - 10 end })

MoveTab:CreateSection("Yetenekler")
MoveTab:CreateToggle({ Name = "U√ßma (Fly)", Callback = function(v) _G.Fly = v end })
MoveTab:CreateToggle({ Name = "Duvar Ge√ßme (Noclip)", Callback = function(v) _G.Noclip = v end })

-- 6. I≈ûINLANMA (Bƒ∞TCOƒ∞N DAHƒ∞L)
TPTab:CreateButton({ Name = "üí∞ Banka 1 (Merkez)", Callback = function() lp.Character:MoveTo(Vector3.new(-10.7, 15.4, 275.0)) end })
TPTab:CreateButton({ Name = "üí∞ Altƒ±n Banka", Callback = function() lp.Character.HumanoidRootPart.CFrame = CFrame.new(-27.9, -51.3, 271.4) end })
TPTab:CreateButton({ Name = "üí∞ Elmas Banka", Callback = function() lp.Character.HumanoidRootPart.CFrame = CFrame.new(-57.1, -50.8, 187.4) end })
