-- KÄ±sayol tuÅŸlarÄ±: RightShift = noclip toggle, RightControl = infjump toggle, RightAlt = save to next slot
local nextSaveSlot = 1
uis.InputBegan:Connect(function(input, gameProcessed)
    if gameProcessed then return end
    if input.KeyCode == Enum.KeyCode.RightShift then
        _G.Noclip = not _G.Noclip
        Rayfield:Notify({Title = "Noclip", Content = tostring(_G.Noclip), Duration = 1.5})
    elseif input.KeyCode == Enum.KeyCode.RightControl then
        _G.InfJump = not _G.InfJump
        Rayfield:Notify({Title = "InfJump", Content = tostring(_G.InfJump), Duration = 1.5})
    elseif input.KeyCode == Enum.KeyCode.RightAlt then
        -- current position'i nextSaveSlot'e kaydet
        if lp.Character and lp.Character:FindFirstChild("HumanoidRootPart") then
            savedPositions[nextSaveSlot] = lp.Character.HumanoidRootPart.CFrame
            Rayfield:Notify({Title = "Pozisyon Kaydedildi", Content = "Slot "..tostring(nextSaveSlot).." kaydedildi", Duration = 2})
            nextSaveSlot = nextSaveSlot + 1
            if nextSaveSlot > 3 then nextSaveSlot = 1 end
        end
    end
end)

