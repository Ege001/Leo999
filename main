ExtraTab:CreateSection("Diƒüer")
ExtraTab:CreateToggle({ Name = "Sonsuz Zƒ±plama", Callback = function(v) _G.InfiniteJump = v end })

-- 9. ANA D√ñNG√ú (STABƒ∞L)
RunService.Heartbeat:Connect(function()
    pcall(function()
        local char = lp.Character
        local hum = char and char:FindFirstChildOfClass("Humanoid")
        local hrp = char and char:FindFirstChild("HumanoidRootPart")
        
        if hum then
            hum.WalkSpeed = _G.SpeedEnabled and _G.Speed or 16
            hum.JumpPower = _G.JumpEnabled and _G.Jump or 50
        end
        
        if _G.Noclip and char then
            for _, p in pairs(char:GetDescendants()) do if p:IsA("BasePart") then p.CanCollide = false end end
        end
        
        if _G.Fly and hrp then
            if not hrp:FindFirstChild("SovFly") then
                local bv = Instance.new("BodyVelocity", hrp); bv.Name = "SovFly"; bv.MaxForce = Vector3.new(9e9,9e9,9e9)
                local bg = Instance.new("BodyGyro", hrp); bg.Name = "SovGyro"; bg.MaxTorque = Vector3.new(9e9,9e9,9e9)
            end
            hrp.SovGyro.CFrame = Camera.CFrame
            local move = Vector3.new(0,0,0)
            if UserInputService:IsKeyDown(Enum.KeyCode.W) then move = move + Camera.CFrame.LookVector end
            if UserInputService:IsKeyDown(Enum.KeyCode.S) then move = move - Camera.CFrame.LookVector end
            if UserInputService:IsKeyDown(Enum.KeyCode.D) then move = move + Camera.CFrame.RightVector end
            if UserInputService:IsKeyDown(Enum.KeyCode.A) then move = move - Camera.CFrame.RightVector end
            hrp.SovFly.Velocity = move * 70
        elseif hrp and hrp:FindFirstChild("SovFly") then
            hrp.SovFly:Destroy(); hrp.SovGyro:Destroy()
        end
    end)
end)

UserInputService.JumpRequest:Connect(function()
    if _G.InfiniteJump and lp.Character then lp.Character:FindFirstChildOfClass("Humanoid"):ChangeState("Jumping") end
end)

-- HUD
local function BuildHUD()
    local sg = Instance.new("ScreenGui", lp.PlayerGui)
    local f = Instance.new("Frame", sg); f.Size = UDim2.new(0, 160, 0, 60); f.Position = UDim2.new(0, 10, 0, 10); f.BackgroundColor3 = Color3.new(0,0,0); f.BackgroundTransparency = 0.5; Instance.new("UICorner", f)
    local l = Instance.new("TextLabel", f); l.Size = UDim2.new(1,0,1,0); l.BackgroundTransparency = 1; l.TextColor3 = Color3.new(1,1,1); l.Font = Enum.Font.Code; l.TextSize = 12
    task.spawn(function()
        while task.wait(0.5) do
            if not sg.Parent then break end
            l.Text = string.format("SOV ULTIMATE V46\nSPD: %d | JMP: %d\nITEM: Hazƒ±r", _G.Speed, _G.Jump)
        end
    end)
end
BuildHUD()

Rayfield:Notify({Title = "Sovereign Ultimate", Content = "Brookhaven E≈üyalarƒ± ve Bitcoin TP Eklendi!", Duration = 5})
