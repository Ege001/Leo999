-- --- [3. AI EÅYA & MONEY GUN] ---
local ToolTab = Window:CreateTab("ğŸ“¦ EÅŸya FabrikasÄ±", 4483362458)

local function GiveToolAI(name)
    pcall(function()
        local tool = rs:FindFirstChild(name, true)
        if tool then
            tool:Clone().Parent = lp.Backpack
            Rayfield:Notify({Title = "AI", Content = name .. " AlÄ±ndÄ±!", Duration = 2})
        else
            -- Remote Event Bypass (Vip EÅŸyalar Ä°Ã§in)
            local r = rs:FindFirstChild("ItemRemote", true) or rs:FindFirstChild("GiveItem", true)
            if r then r:FireServer(name) end
        end
    end)
end

ToolTab:CreateButton({Name = "ğŸ’µ MONEY GUN (VÄ°P) AL", Callback = function() GiveToolAI("MoneyGun") end})
ToolTab:CreateButton({Name = "ğŸ”‘ KEYCARD AL", Callback = function() GiveToolAI("KeyCard") end})

