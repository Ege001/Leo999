-- 7. E≈ûYALAR (BROOKHAVEN √ñZEL)
ToolTab:CreateSection("Envantere E≈üya Al")

local bhTools = {
    "MoneyGun", "Hammer", "Pizza", "KeyCard", "Camera", "Smartphone", "Briefcase", "Flashlight", "Binoculars", "SleepingBag"
}

ToolTab:CreateDropdown({
    Name = "E≈üya Se√ß",
    Options = bhTools,
    CurrentOption = "MoneyGun",
    Callback = function(Option)
        pcall(function()
            -- Brookhaven e≈üya verme sistemi (RemoteEvent √ºzerinden)
            local remote = ReplicatedStorage:FindFirstChild("ItemRemote", true) or ReplicatedStorage:FindFirstChild("GiveItem", true)
            if remote then
                remote:FireServer(Option)
            end
            
            -- Manuel Clone (Eƒüer remote √ßalƒ±≈ümazsa)
            local tool = ReplicatedStorage:FindFirstChild(Option, true)
            if tool and tool:IsA("Tool") then
                tool:Clone().Parent = lp.Backpack
            end
            
            Rayfield:Notify({Title = "E≈üya Alƒ±ndƒ±", Content = Option .. " envanterine eklendi!", Duration = 2})
        end)
    end,
})

ToolTab:CreateButton({
    Name = "üíµ Hƒ±zlƒ± Money Gun Al",
    Callback = function()
        pcall(function()
            local tool = ReplicatedStorage:FindFirstChild("MoneyGun", true)
            if tool then tool:Clone().Parent = lp.Backpack end
        end)
    end,
})

